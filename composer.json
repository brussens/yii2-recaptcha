{
  "name": "brussens/yii2-recaptcha",
  "description": "Google ReCaptcha v2.0 Yii 2.x.x extension",
  "license": "MIT",
  "type": "yii2-extension",
  "version":"1.1.0",
  "keywords": ["yii2", "yii", "recaptcha", "google"],
  "authors": [
    {
      "name": "Brusensky Dmitry",
      "email": "brussens@nativeweb.ru"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "google/recaptcha": "~1.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "brussens\\yii2\\extensions\\recaptcha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "brussens\\yii2\\extensions\\recaptcha\\test\\": "test/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
